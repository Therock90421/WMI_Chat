<body class="light-blue-skin" aria-busy="true">
  <%= render 'homes/header' %>

  <main>
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6">
          <div class="modal-dialog cascading-modal" role="document">
            <div class="modal-content" style="background-color: rgb(255, 255, 255,.8);">
              <div class="modal-header darken-3 white-text waves-effect waves-light" style="background-color: rgb(46, 139, 87);">
                <h4 class="title"><i class="	fa fa-id-card-o"></i> SIGN UP</h4>
              </div>
              <%= render 'users/register' %>
            </div>
          </div>
        </div>
        <div class="col-md-3"></div>
      </div>
    </div>
  </main>

  <%= render 'layouts/footer' %>

</body>